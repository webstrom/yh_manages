doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport"   content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
        title 每日数据统计
    body
        div.yh_manage_account#manages_statistics_advanceThaw
            include ../../common/statistics.pug
            div.yh_operation
                a excel导出
                a 冻结解冻
                div.yh_role_search.yh_user_right_input.yh_census_input
                    input(type='text' placeholder='机构名' onfocus="this.placeholder=''" onblur="this.placeholder='机构名'")
                    s
                        input.laydate-icon.form-control.layer-date(type='text' placeholder='渠道' onfocus="this.placeholder=''" onblur="this.placeholder='渠道'")
                        i.iconfont.icon-riqi
                    s
                        input(type='text' placeholder='开始时间' onfocus="this.placeholder=''" onblur="this.placeholder='开始时间'" id='manages_time')
                        i.iconfont.icon-riqi
                    s
                        input(type='text' placeholder='截止时间' onfocus="this.placeholder=''" onblur="this.placeholder='截止时间'" id='manages_time_1')
                        i.iconfont.icon-riqi
                    a
                        i.iconfont.icon-search
                        em 搜索
            div.yh_table
                table
                    thead
                        tr
