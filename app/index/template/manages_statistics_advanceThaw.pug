                            th 编号
                            th 时间
                            th 机构码
                            th 注册数
                            th 申请订单数
                            th 通过订单数
                            th 线上放款金额
                            th 线下放款金额
                            th 线上还款金额
                            th 线下还款金额
                            th 退票金额
                            th 逾期订单数
                            th 逾期90天以上订单
                            th 后台充值金额
                            th 后台提现金额
                            th 服务费金额
                    tbody
                        tr(v-for='(item,index) in checkboxData')
                            td
                                b
                                    label
                                        input(:id="'input-select-'+index" :name="'input-select-'+index" class="color-input-red" type="checkbox" v-model='checkboxList' :value='index')
                                        label(:for="'input-select-'+index")
                            td(v-html='item.sik')
                            td(v-html='item.role')
                            td(v-html='item.name')
                            td(v-html='item.sbm')
                            td(v-html='item.dis')
                            td(v-html='item.dd')
                            td(v-html='item.xs')
                            td(v-html='item.xx')
                            td(v-html='item.xshk')
                            td(v-html='item.xxhk')
                            td(v-html='item.tp')
                            td(v-html='item.yq')
                            td(v-html='item.yqts')
                            td(v-html='item.ht')
                            td(v-html='item.tx')
                            td(v-html='item.fw')
                div.yh_manages_checked
                    div#yh_manages_lauerpage
                    div.yh_checked_box
                        label
                            input(id='input-id' class="color-input-red" type="checkbox" name="input-id" value="" v-model='checked' @click='checkedAll')
                            label(for="input-id")
                            span 全选
                        span 删除